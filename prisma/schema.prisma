// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model TransactionLineItem {
  id          String   @id @default(cuid())
  name        String
  date        DateTime
  amount      Int
  category    String
  description String
}
